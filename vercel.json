{
  "version": 2,
  "builds": [
    {
      "src": "dashboard/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dashboard/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dashboard/build/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
